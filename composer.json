{
    "name": "gerardojbaez/laraplans",
    "description": "SaaS style recurring plans for Laravel.",
    "keywords": ["plans", "laravel", "subscriptions", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.18|^10.0",
        "illuminate/database": "^9.18|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.7|^8.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.5.24",
        "laravel/pint": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Gerardojbaez\\Laraplans\\": "src/",
            "Gerardojbaez\\Laraplans\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Gerardojbaez\\Laraplans\\Tests\\": "tests/",
            "Gerardojbaez\\Laraplans\\Database\\Factories\\": "database/factories/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
